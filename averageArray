using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace averageArray
{
    class Program
    {
        static void Main(string[] args)
        {

            int[] numbers = { 10,10,7};
            int sum = 0;

            for (int i = 0; i < numbers.Length; i++)
            {
                sum += numbers[i];
            }
            double avgArry = sum / numbers.Length;
            Console.WriteLine(avgArry);
            Console.ReadKey();

        }
    }
}
